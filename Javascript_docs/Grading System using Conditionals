//Write a program to determine the grade of the student
//Determine the grade and assign it to the result variable.
//Do not alter anything else given in the starter code

function main(marks) {
    let result;

    if (marks >= 90) {
        result = "A grade";
    } else if (marks >= 80 && marks < 90) {
        result = "B grade";
    } else if (marks >= 70 && marks < 80) {
        result = "C grade";
    } else if (marks >= 60 && marks < 70) {
        result = "D grade";
    } else {
        result = "F grade";
    }
    return result;
}

// Example usage
const studentMarks = 85;
const grade = main(studentMarks);
console.log(`Student received ${grade} for marks: ${studentMarks}`);

