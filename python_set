s={}

type(s)

s.add(1)

s

def sumUnique(l):
    s=set()
    for i in l:
        s.add(i)
    sum=0
    for i in s:
        sum=sum+i
    return sum

sumUnique([1,2,3,4,3,4,2,1,5,5])
